{
    "name": "beebmx/kirby-rickroll",
    "description": "Kirby Rickroll",
    "keywords": [
        "kirby", "kirby-3", "kirby3", "kirby-cms", "kirby-plugin", "rickroll", "kirby-rickroll"
    ],
    "type": "kirby-plugin",
    "version": "1.0.0",
    "license": "MIT",
    "authors": [
        {
            "name": "Fernando Gutierrez",
            "email": "fernando@beeb.mx"
        }
    ],
    "require": {
        "php": "^7.2",
        "getkirby/composer-installer": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "getkirby/cms": "^3.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Beebmx\\KirbyRickroll\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Beebmx\\KirbyRickroll\\Tests\\": "tests/"
        }
    },
    "extra": {
        "installer-name": "kirby-rickroll"
    }
}
